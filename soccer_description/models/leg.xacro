<robot name="soccerbot" xmlns:xacro="http://www.ros.org/wiki/xacro">
    <xacro:macro name="leg" params="name origin direction">
        <link name="${name}_hip_side">
            <inertial>
                <mass value="9.364e-3"/>
                <origin rpy="0  0  0" xyz="0.0003695 0.0000 0.01689"/>
                <inertia ixx="0.002e-3" ixy="0.000" ixz="0.000"
                         iyy="0.005e-3" iyz="0.000"
                         izz="0.004e-3"/>
            </inertial>
            <collision name="collision">
                <origin rpy="0  0  0" xyz="0 0 0.00899"/>
                <geometry>
                    <box size="0.0485 0.022 0.0039"/>
                </geometry>
            </collision>
            <visual name="visual">
                <origin rpy="0  0  0" xyz="0  0  0"/>
                <geometry>
                    <mesh filename="${FILE_PATH}/meshes/${name}_hip_side.${MODEL_TYPE}" scale="1 1 1"/>
                </geometry>
                <material name="black" />
            </visual>
        </link>
        <link name="${name}_hip_front">
            <inertial>
                <mass value="153.186e-3"/>
                <origin rpy="0 0 0" xyz="-0.02093 0 -0.011313"/>
                <inertia ixx="0.041e-3" ixy="0.000" ixz="-0.002e-3"
                         iyy="0.118e-3" iyz="0.000"
                         izz="0.106e-3"/>
            </inertial>
            <collision name="collision">
                <origin rpy="0 0 0" xyz="-0.02187 0 -0.01332"/>
                <geometry>
                    <box size="0.083747 0.04885 0.0506"/>
                </geometry>
            </collision>
            <visual name="visual">
                <origin rpy="0  0  0" xyz="0  0  0"/>
                <geometry>
                    <mesh filename="${FILE_PATH}/meshes/${name}_hip_front.${MODEL_TYPE}" scale="1 1 1"/>
                </geometry>
                <material name="black" />
            </visual>
        </link>
        <link name="${name}_thigh">
            <inertial>
                <mass value="21.438e-3"/>
                <origin rpy="0 0 0" xyz="0.0019422 ${direction * 0.0001419} -0.0469409"/>
                <inertia ixx="0.025e-3" ixy="${direction * 0.002e-3}" ixz="0.000"
                         iyy="0.016e-3" iyz="0.000"
                         izz="0.010e-3"/>
            </inertial>
            <collision name="collision">
                <origin rpy="0 0 0" xyz="0 0 -0.0445"/>
                <geometry>
                    <box size="0.022 0.048 0.111"/>
                </geometry>
            </collision>
            <visual name="visual">
                <origin rpy="0  0  0" xyz="0  0  0"/>
                <geometry>
                    <mesh filename="${FILE_PATH}/meshes/${name}_thigh.${MODEL_TYPE}" scale="1 1 1"/>
                </geometry>
                <material name="black" />
            </visual>
        </link>
        <link name="${name}_calve">
            <inertial>
                <mass value="86.547e-3"/>
                <origin rpy="0  0  0" xyz="0 ${direction * -0.0003259e-3} -0.0150123e-3"/>
                <inertia ixx="0.054e-3" ixy="0.000" ixz="0.000"
                         iyy="0.046e-3" iyz="-0.001e-3"
                         izz="0.019e-3"/>
            </inertial>
            <collision name="collision">
                <origin rpy="0 0 0" xyz="0 0 -0.03385"/>
                <geometry>
                    <box size="0.031883 0.048 0.105683"/>
                </geometry>
            </collision>
            <visual name="visual">
                <origin rpy="0 0 0" xyz="0 0 -0.0827"/>
                <geometry>
                    <mesh filename="${FILE_PATH}/meshes/${name}_calve.${MODEL_TYPE}" scale="1 1 1"/>
                </geometry>
                <material name="black" />
            </visual>
        </link>
        <link name="${name}_ankle">
            <inertial>
                <mass value="153.186e-3"/>
                <origin rpy="0 0 0" xyz="-0.020939e-3 0.000 0.011313e-3"/>
                <inertia ixx="0.041e-3" ixy="0.000" ixz="0.002e-3"
                         iyy="0.118e-3" iyz="0.000"
                         izz="0.106e-3"/>
            </inertial>
            <collision name="collision">
                <origin rpy="0 0 0" xyz="-0.02014 0 0.01031"/>
                <geometry>
                    <box size="0.090769 0.042 0.0506"/>
                </geometry>
            </collision>
            <visual name="visual">
                <origin rpy="0 0 0" xyz="0  0  0"/>
                <geometry>
                    <mesh filename="${FILE_PATH}/meshes/${name}_ankle.${MODEL_TYPE}" scale="1 1 1"/>
                </geometry>
                <material name="black" />
            </visual>
        </link>
        <link name="${name}_foot">
            <inertial>
                <mass value="23.216e-3"/>
                <origin rpy="0 0 0" xyz="-0.020939 0.0 0.011313"/>
                <inertia ixx="0.006e-3" ixy="0.000" ixz="0.000"
                         iyy="0.018e-3" iyz="0.000"
                         izz="0.022e-3"/>
            </inertial>
            <collision name="collision">
                <origin rpy="0 0 0" xyz="0.00385 ${direction * -0.00401} -0.00737 "/>
                <geometry>
                    <box size="0.09 0.07 0.03674"/>
                </geometry>
                <contact_coefficients mu ="100.0" />
            </collision>
            <visual name="visual">
                <origin rpy="0  0  0" xyz="0  0  0"/>
                <geometry>
                    <mesh filename="${FILE_PATH}/meshes/${name}_foot.${MODEL_TYPE}" scale="1 1 1"/>
                </geometry>
                <material name="black" />
            </visual>
        </link>

        <joint name="${name}_leg_motor_0" type="revolute">
            <parent link="torso"/>
            <child link="${name}_hip_side"/>
            <origin rpy="0 0 0" xyz="0.0135 ${direction * 0.035} -0.156"/>
            <axis xyz="0 0 ${direction * -1}"/>
            <dynamics damping="${JOINT_DAMPING}" friction="${JOINT_FRICTION}" />
            <limit effort="${MX28_TORQUE}" lower="-1.309" upper="0.524" velocity="${MX28_VELOCITY}"/>
        </joint>
        <joint name="${name}_leg_motor_1" type="revolute">
            <parent link="${name}_hip_side"/>
            <child link="${name}_hip_front"/>
            <origin rpy="0 0 0" xyz="0 0 0"/>
            <axis xyz="${direction} 0 0"/>
            <dynamics damping="${JOINT_DAMPING}" friction="${JOINT_FRICTION}" />
            <limit effort="${MX28_TORQUE}" lower="-${PI/4}" upper="${PI/2}" velocity="${MX28_VELOCITY}"/>
        </joint>
        <joint name="${name}_leg_motor_2" type="revolute">
            <parent link="${name}_hip_front"/>
            <child link="${name}_thigh"/>
            <origin rpy="0 0 0" xyz="0 0 0"/>
            <axis xyz="0 -1 0"/>
            <dynamics damping="${JOINT_DAMPING}" friction="${JOINT_FRICTION}" />
            <limit effort="${MX28_TORQUE}" lower="-${PI/4}" upper="${PI*3/4}" velocity="${MX28_VELOCITY}"/>
        </joint>
        <joint name="${name}_leg_motor_3" type="revolute">
            <parent link="${name}_thigh"/>
            <child link="${name}_calve"/>
            <origin rpy="0 0 0" xyz="0 0 -0.0890"/>
            <axis xyz="0 -1 0"/>
            <dynamics damping="${JOINT_DAMPING}" friction="${JOINT_FRICTION}" />
            <limit effort="${MX28_TORQUE}" lower="-2.793" upper="0" velocity="${MX28_VELOCITY}"/>
        </joint>
        <joint name="${name}_leg_motor_4" type="revolute">
            <parent link="${name}_calve"/>
            <child link="${name}_ankle"/>
            <origin rpy="0 0 0" xyz="0 0 -0.0827"/>
            <axis xyz="0 -1 0"/>
            <dynamics damping="${JOINT_DAMPING}" friction="${JOINT_FRICTION}" />
            <limit effort="${MX28_TORQUE}" lower="-${PI/4}" upper="${PI/2}" velocity="${MX28_VELOCITY}"/>
        </joint>
        <joint name="${name}_leg_motor_5" type="revolute">
            <parent link="${name}_ankle"/>
            <child link="${name}_foot"/>
            <origin rpy="0 0 0" xyz="0 0 0"/>
            <axis xyz="${direction} 0 0"/>
            <dynamics damping="${JOINT_DAMPING}" friction="${JOINT_FRICTION}" />
            <limit effort="${MX28_TORQUE}" lower="-${PI/4}" upper="${PI/4}" velocity="${MX28_VELOCITY}"/>
        </joint>

        <transmission name="${name}_leg_motor_0">
            <type>transmission_interface/SimpleTransmission</type>
            <joint name="${name}_leg_motor_0">
                <hardwareInterface>hardware_interface/EffortJointInterface</hardwareInterface>
            </joint>
            <actuator name="${name}_leg_motor_0">
                <hardwareInterface>hardware_interface/EffortJointInterface</hardwareInterface>
                <mechanicalReduction>1</mechanicalReduction>
            </actuator>
        </transmission>
        <transmission name="${name}_leg_motor_1">
            <type>transmission_interface/SimpleTransmission</type>
            <joint name="${name}_leg_motor_1">
                <hardwareInterface>hardware_interface/EffortJointInterface</hardwareInterface>
            </joint>
            <actuator name="${name}_leg_motor_1">
                <hardwareInterface>hardware_interface/EffortJointInterface</hardwareInterface>
                <mechanicalReduction>1</mechanicalReduction>
            </actuator>
        </transmission>
        <transmission name="${name}_leg_motor_2">
            <type>transmission_interface/SimpleTransmission</type>
            <joint name="${name}_leg_motor_2">
                <hardwareInterface>hardware_interface/EffortJointInterface</hardwareInterface>
            </joint>
            <actuator name="${name}_leg_motor_2">
                <hardwareInterface>hardware_interface/EffortJointInterface</hardwareInterface>
                <mechanicalReduction>1</mechanicalReduction>
            </actuator>
        </transmission>
        <transmission name="${name}_leg_motor_3">
            <type>transmission_interface/SimpleTransmission</type>
            <joint name="${name}_leg_motor_3">
                <hardwareInterface>hardware_interface/EffortJointInterface</hardwareInterface>
            </joint>
            <actuator name="${name}_leg_motor_3">
                <hardwareInterface>hardware_interface/EffortJointInterface</hardwareInterface>
                <mechanicalReduction>1</mechanicalReduction>
            </actuator>
        </transmission>
        <transmission name="${name}_leg_motor_4">
            <type>transmission_interface/SimpleTransmission</type>
            <joint name="${name}_leg_motor_4">
                <hardwareInterface>hardware_interface/EffortJointInterface</hardwareInterface>
            </joint>
            <actuator name="${name}_leg_motor_4">
                <hardwareInterface>hardware_interface/EffortJointInterface</hardwareInterface>
                <mechanicalReduction>1</mechanicalReduction>
            </actuator>
        </transmission>
        <transmission name="${name}_leg_motor_5">
            <type>transmission_interface/SimpleTransmission</type>
            <joint name="${name}_leg_motor_5">
                <hardwareInterface>hardware_interface/EffortJointInterface</hardwareInterface>
            </joint>
            <actuator name="${name}_leg_motor_5">
                <hardwareInterface>hardware_interface/EffortJointInterface</hardwareInterface>
                <mechanicalReduction>1</mechanicalReduction>
            </actuator>
        </transmission>

        <gazebo reference="${name}_hip_side">
            <kp>${JOINT_KP}</kp>
            <kd>${JOINT_KD}</kd>
            <mu1>${JOINT_MU1}</mu1>
            <mu2>${JOINT_MU2}</mu2>
        </gazebo>
        <gazebo reference="${name}_hip_front">
            <kp>${JOINT_KP}</kp>
            <kd>${JOINT_KD}</kd>
            <mu1>${JOINT_MU1}</mu1>
            <mu2>${JOINT_MU2}</mu2>
        </gazebo>
        <gazebo reference="${name}_thigh">
            <kp>${JOINT_KP}</kp>
            <kd>${JOINT_KD}</kd>
            <mu1>${JOINT_MU1}</mu1>
            <mu2>${JOINT_MU2}</mu2>
        </gazebo>
        <gazebo reference="${name}_calve">
            <kp>${JOINT_KP}</kp>
            <kd>${JOINT_KD}</kd>
            <mu1>${JOINT_MU1}</mu1>
            <mu2>${JOINT_MU2}</mu2>
        </gazebo>
        <gazebo reference="${name}_ankle">
            <kp>${JOINT_KP}</kp>
            <kd>${JOINT_KD}</kd>
            <mu1>${JOINT_MU1}</mu1>
            <mu2>${JOINT_MU2}</mu2>
        </gazebo>
        <gazebo reference="${name}_foot">
            <mu1>${FOREARM_MU1}</mu1>
            <mu2>${FOREARM_MU2}</mu2>
        </gazebo>
    </xacro:macro>
</robot>
