component PS_q_axs_to_qz   
% PS Calculate qz
% This block calculates the rotational angle about the z axis using Angle
% and Axis outputs of the Transform Sensor block

% Copyright 2016-2017 The MathWorks, Inc.

inputs
    q = {0, 'rad'}; % q:left
    axs = {[0 0 0]','1'} %axs:left
end

outputs
    qz = {0, 'rad'}; %qz:right
end

equations
	qz == q*sign(axs(3));
end

end
