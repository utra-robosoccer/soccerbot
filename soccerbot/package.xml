<?xml version="1.0"?>
<package format="2">
  <name>soccerbot</name>
  <version>0.0.0</version>
  <description>Main package to run soccerbot</description>

  <maintainer email="vuwij@me.com">Jason Wang</maintainer>
  <license>BSD</license>

  <buildtool_depend>catkin</buildtool_depend>
  <depend>xterm</depend>

  <!-- Simulation -->
  <depend>bez1_description</depend>
  <depend>bez2_description</depend>
  <depend>bez3_description</depend>
  <!-- Sensors -->
  <depend>usb_cam</depend>
  <depend>soccer_hardware</depend>
  <depend>soccer_hardware_bez3</depend>

<!--  <depend>soccer_embedded</depend>-->

  <!-- Localization -->
  <depend>map_server</depend>
  <depend>soccer_object_detection</depend>
  <depend>soccer_object_localization</depend>
  <depend>soccer_common</depend>
  <depend>imu_complementary_filter</depend>
  <depend>robot_pose_ekf</depend>
  <depend>robot_localization</depend>
  <depend>soccer_pycontrol</depend>
  <depend>soccer_localization</depend>
  <depend>webots_ros</depend>
  <exec_depend>amcl</exec_depend>
  <exec_depend>pointcloud_to_laserscan</exec_depend>

  <!-- Navigation-->
  <depend>soccer_trajectories</depend>
  <depend>soccer_firmware_interface</depend>
<!--  <depend>soccer_webots</depend>-->
  <depend>soccer_msgs</depend>
  <depend>python3-protobuf</depend>

  <!-- Communication-->

  <!-- strategy-->
  <depend>soccer_strategy</depend>

  <!-- Game Controller -->
  <depend>java</depend>
</package>
