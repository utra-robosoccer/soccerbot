<launch>
    <arg name="goalie" default="true" />
    <arg name="shared_strategy_source" value="$(optenv SINGLE_STRATEGY_SOURCE true) "/>
    <env name="ROBOCUP_TEAM_ID" value="$(optenv ROBOCUP_TEAM_ID 16)" />
    <arg name="ROBOCUP_TEAM_COLOR" default="red" />
<!--    <env name="ROBOCUP_TEAM_COLOR" value="$(optenv ROBOCUP_TEAM_COLOR red)" />-->
    <env name="ROBOCUP_GAMECONTROLLER_IP" value="$(optenv ROBOCUP_GAMECONTROLLER_IP 0.0.0.0)" />
    <rosparam param="ROBOCUP_TEAM_COLOR" subst_value="true" >$(arg ROBOCUP_TEAM_COLOR)</rosparam>
    <node pkg="soccer_strategy" type="main.py" name="soccer_strategy" output="screen" respawn="true"/>
</launch>
